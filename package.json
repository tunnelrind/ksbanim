{
    "name": "ksbanim",
    "displayName": "ksbanim",
    "description": "an extension to download ksbanim and PyQt5",
    "version": "1.0.0",
    "publisher": "ksb",
    "author": {
      "name": "Dario Biasini"
    },
    "engines": {
      "vscode": "^1.60.0"
    },
    "categories": [
      "Education",
      "Other"
    ],
    "icon": "icon.png",
    "activationEvents": [
    ],
    "license": "GPL-3.0-or-later",
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "ksbanim.downloadFile",
          "title": "ksbanim.py"
        },
        {
          "command": "ksbanim.downloadQt",
          "title": "PyQt5"
        }
      ],
      "views": {
        "explorer": [
          {
            "id": "ksbanim.myCustomView",
            "name": "ksbanim"
          }
        ]
      }
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
      "typescript": "^4.0.0",
      "vscode": "^1.1.37",
      "@types/node": "^14.14.37"
    }
  }