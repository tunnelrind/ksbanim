{
  "name": "my-vscode-extension",
  "displayName": "My VSCode Extension",
  "description": "A VSCode extension that provides ksbanim.py module.",
  "version": "0.0.1",
  "engines": {
      "vscode": "^1.60.0"
  },
  "categories": [
      "Other"
  ],
  "main": "extension.js",
  "contributes": {
      "commands": [
          {
              "command": "extension.installPyQt5",
              "title": "Install PyQt5"
          },
          {
              "command": "extension.downloadModule",
              "title": "Download ksbanim.py"
          }
      ]
  },
  "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./"
  },
  "devDependencies": {
      "typescript": "^4.0.0",
      "@types/node": "^14.0.0",
      "@types/vscode": "^1.60.0"
  }
}